
{
  "version": 2,
  "rewrites": [
    {
      "source": "/mineru-proxy/:path*",
      "destination": "https://mineru.net/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
